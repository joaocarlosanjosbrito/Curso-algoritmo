algoritmo "semnome"
var
   dinheiro: inteiro

inicio
        Escreva ("Quando eu tenho de dinheiro ")
        Leia(dinheiro)
    se (dinheiro >= 10000) entao
        Escreva ("Partiu Disney")
    senao
    
      se (dinheiro >= 5000) e (dinheiro < 10000) entao
        Escreva ("Visitar família")
     senao
        Escreva ("Tõ liso ")
     FimSe
     
    FimSe
fimalgoritmo
